{
  "name": "eusa-api-upgrade",
  "version": "0.1.0",
  "description": "EU/SA API Project",
  "author": "Cognus",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:test": "cross-env NODE_ENV=test nodemon",
    "prestart:prod": "rm -rf dist && tsc",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "cross-env NODE_ENV=test npm run migrate down && cross-env NODE_ENV=test npm run migrate up && cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js",
    "migrate": "ts-node migrate.ts",
    "migrate:test": "cross-env NODE_ENV=test ts-node migrate.ts"
  },
  "dependencies": {
    "@nest-middlewares/helmet": "^5.0.0",
    "@nest-middlewares/morgan": "^5.0.0",
    "@nestjs/common": "^5.0.0",
    "@nestjs/core": "^5.0.0",
    "@nestjs/swagger": "^2.2.0",
    "@types/winston": "^2.3.9",
    "bcrypt-nodejs": "0.0.3",
    "class-transformer": "^0.1.9",
    "class-validator": "^0.9.1",
    "dotenv": "^6.0.0",
    "fastify-formbody": "^2.0.0",
    "firebase-admin": "^5.13.0",
    "jsonwebtoken": "^8.3.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.6.0-forward-compat.5",
    "sequelize": "^4.38.0",
    "sequelize-typescript": "^0.6.6-beta.1",
    "typescript": "^2.6.2",
    "umzug": "^2.1.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.1.0",
    "@types/express": "^4.0.39",
    "@types/jest": "^21.1.8",
    "@types/node": "^9.3.0",
    "@types/sequelize": "^4.27.24",
    "@types/supertest": "^2.0.4",
    "cross-env": "^5.2.0",
    "jest": "^21.2.1",
    "nodemon": "^1.14.1",
    "prettier": "^1.11.1",
    "supertest": "^3.0.0",
    "ts-jest": "^21.2.4",
    "ts-loader": "^4.1.0",
    "ts-node": "^4.1.0",
    "tsconfig-paths": "^3.1.1",
    "tslint": "5.3.2",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
